<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://jboss.org/schema/arquillian"
xsi:schemaLocation="http://jboss.org/schema/arquillian http://jboss.org/schema/arquillian/arquillian_1_0.xsd">

	<engine>
		<property name="deploymentExportPath">target/</property>
	</engine>
	
	<container qualifier="liberty-remote">
		<container qualifier="websphere" default="true">
        <configuration>
        	<property name="hostName">arquillianremotetestserver.mybluemix.net</property>
			<property name="serverName">LibertyProjectServer</property>
			<property name="username">admin</property>
			<property name="password">admin</property>
			<property name="httpPort">9080</property>
			<property name="httpsPort">9443</property>
            <property name="outputToConsole">true</property>
            <property name="appDeployTimeout">600</property>
        </configuration>
	</container>
</arquillian>